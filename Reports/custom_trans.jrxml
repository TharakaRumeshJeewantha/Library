		<queryString language="SQL">
			<![CDATA[select count(tid) as tot 
from tbl_transactions t, tbl_students s
where t_status = "done"
AND s.std_reg_no = t.std_no
AND (t.bk_issued_date <=  $P{P_issued_date}  OR  $P{P_issued_date}  IS NULL) 
AND (t.bk_returned_date >=  $P{P_returned_date}   OR  $P{P_returned_date}    IS NULL)  
AND (s.teacher >=  $P{teacher}   OR  $P{teacher}    IS NULL)  
AND ( s.std_grade >= $P{grade}    OR   $P{grade}  IS NULL)  
AND (s.std_class >=  $P{class}   OR  $P{class}    IS NULL)]]>
		</queryString>
		<field name="tot" class="java.lang.Long">
			<property name="com.jaspersoft.studio.field.label" value="tot"/>
		</field>
