	<queryString language="SQL">
		<![CDATA[select b.bk_no, b.bk_category_no, b.bk_name, b.bk_author,
s.std_reg_no,s.std_fname, s.std_lname, s.teacher, s.std_grade, s.std_class,
t.bk_issued_date, t.bk_should_return_date, t.bk_returned_date
from tbl_transactions t, tbl_students s, tbl_books b
where 
